 device/fsl/imx6/imx6.mk                       |   2 ++
 device/fsl/smarc/BoardConfig.mk               |   2 ++
 device/fsl/smarc/apk/SmarcOTA.apk             | Bin 0 -> 21799 bytes
 kernel_imx/arch/arm/mach-mx6/smarc/rev_sa01.c |   6 +++---
 kernel_imx/arch/arm/mach-mx6/smarc/rev_sa01.h |  15 +++++++--------
 6 files changed, 18 insertions(+), 15 deletions(-)
 create mode 100644 device/fsl/smarc/apk/SmarcOTA.apk

diff --git a/build/core/Makefile b/build/core/Makefile
index 2eb528d..1b72619 100644
--- a/build/core/Makefile
+++ b/build/core/Makefile
@@ -19,10 +19,10 @@ is_tests_build := $(filter tests,$(MAKECMDGOALS))
 # The filter part means "only eval the copy-one-file rule if this
 # src:dest pair is the first one to match the same dest"
 #$(1): the src:dest pair
-define check-product-copy-files
-$(if $(filter %.apk, $(1)),$(error \
-    Prebuilt apk found in PRODUCT_COPY_FILES: $(1), use BUILD_PREBUILT instead!))
-endef
+#define check-product-copy-files
+#$(if $(filter %.apk, $(1)),$(error \
+#    Prebuilt apk found in PRODUCT_COPY_FILES: $(1), use BUILD_PREBUILT instead!))
+#endef
 # filter out the duplicate <source file>:<dest file> pairs.
 unique_product_copy_files_pairs :=
 $(foreach cf,$(PRODUCT_COPY_FILES), \
diff --git a/device/fsl/imx6/imx6.mk b/device/fsl/imx6/imx6.mk
index 76d41d2..b65abcd 100644
--- a/device/fsl/imx6/imx6.mk
+++ b/device/fsl/imx6/imx6.mk
@@ -16,6 +16,8 @@ PRODUCT_PACKAGES += \
 	Camera					\
 	LegacyCamera                            \
 	Email					\
+	libAesLIB				\
+	HdmiDualPlayback			\
 	CactusPlayer                            \
 	WfdSink                                 \
 	wfd                                     \
diff --git a/device/fsl/smarc/BoardConfig.mk b/device/fsl/smarc/BoardConfig.mk
index 238d8ad..21f3a97 100644
--- a/device/fsl/smarc/BoardConfig.mk
+++ b/device/fsl/smarc/BoardConfig.mk
@@ -22,6 +22,8 @@ PRODUCT_COPY_FILES +=	\
 	device/fsl/smarc/fstab.freescale:root/fstab.freescale
 endif # BUILD_TARGET_FS
 
+PRODUCT_COPY_FILES +=	\
+	device/fsl/smarc/apk/SmarcOTA.apk:system/app/SmarcOTA.apk
 
 TARGET_BOOTLOADER_BOARD_NAME := SMARC
 PRODUCT_MODEL := SMARC-MX6DQ
