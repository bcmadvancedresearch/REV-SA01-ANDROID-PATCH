new file mode 100755
index 0000000..e4f61c8
--- /dev/null
+++ b/kernel_imx/run.sh
@@ -0,0 +1,33 @@
+#!/bin/bash
+
+LOCAL_PATH=`pwd`
+ARCH=arm
+CROSS_COMPILE=$LOCAL_PATH/../prebuilts/gcc/linux-x86/arm/arm-eabi-4.6/bin/arm-eabi-
+JOBS=$1
+
+if [ $1 == "distclean" ]; then
+	make ARCH=$ARCH CROSS_COMPILE=$CROSS_COMPILE distclean
+	exit;
+fi
+
+if [ $1 == "clean" ]; then
+	make ARCH=$ARCH CROSS_COMPILE=$CROSS_COMPILE clean
+	exit;
+fi
+
+if [ $1 == "android" ]; then
+	make ARCH=$ARCH CROSS_COMPILE=$CROSS_COMPILE imx6_android_defconfig
+	JOBS=$2
+fi
+
+if [ $1 == "smarc" ]; then
+	make ARCH=$ARCH CROSS_COMPILE=$CROSS_COMPILE imx6_smarc_android_defconfig
+	JOBS=$2
+fi
+
+if [ $1 == "menuconfig" ]; then
+	make ARCH=$ARCH CROSS_COMPILE=$CROSS_COMPILE menuconfig
+	exit;
+fi
+
+make ARCH=$ARCH CROSS_COMPILE=$CROSS_COMPILE KERNELRELEASE=3.0.35-2310 uImage $JOBS
diff --git a/run.sh b/run.sh
new file mode 100755
index 0000000..1282039
--- /dev/null
+++ b/run.sh
@@ -0,0 +1,25 @@
+#!/bin/bash
+
+if [ $1 == "clean" ]; then
+	echo "<< Remove out/ >>"
+    rm -rf out
+	echo "<< Remove wi-fi tmp files >>"
+    rm -r hardware/imx/wlan/atheros
+    git checkout hardware/imx/wlan/atheros
+	echo "<< clean kernel >>"
+	cd kernel_imx
+    ./run.sh distclean
+	rm firmware/imx/sdma/sdma-imx6q-to1.bin
+	echo "<< clean u-boot >>"
+    cd ../bootable/bootloader/uboot-imx/
+	./run.sh distclean
+    cd ../../../
+	exit;
+fi
+
+MODE=eng
+
+source build/envsetup.sh
+lunch smarc-${MODE}
+time make $1 $2 $3
+
